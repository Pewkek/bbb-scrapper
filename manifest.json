{
	"name": "BBB Presentation Grabber",
	"description": "Opens currently shown presentation on BigBlueButton in a new tab, with printing support",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"icons": {
		"16": "icon/icon16.png",
		"32": "icon/icon32.png",
		"64": "icon/icon64.png",
		"128": "icon/icon128.png"
	},
	"permissions": ["activeTab", "scripting", "declarativeContent" ],
	"action": {
		"default_icon": {
			"16": "icon/icon_disabled16.png",
			"32": "icon/icon_disabled32.png",
			"64": "icon/icon_disabled64.png",
			"128": "icon/icon_disabled128.png"
		},
		"default_title": "Grab presentation"
	}
}